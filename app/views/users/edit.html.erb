<h1>User info</h1>
　<!--users editから-->
<%= form_with model: @user do |f| %>

  <div>Name</div>
  <%= f.text_field :name %>
  
  <div>Image</div>
  <%= f.file_field :profile_image, accept: "image/*" %>
  
  <div>Introduction</div>
  <%= f.text_field :introduction %>
  <br>
  <%= f.submit "Update User" %>
<% end %>
